@using System.Web.Optimization

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" type="image/png" sizes="16x16" href="../plugins/images/favicon.png">
    <title>Elite Admin Template - The Ultimate Multipurpose admin template</title>
    <!-- Bootstrap Core CSS -->
    @Styles.Render("~/bundles/adminstyles")
    <!-- Custom CSS -->
    <link href="~/AdminTemplate/css/style.min.css" rel="stylesheet" />
    <!-- color CSS -->
    <link href="~/AdminTemplate/css/colors/default.min.css" rel="stylesheet" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <section id="wrapper" class="error-page">
        <div class="error-box">
            <div class="error-body text-center">
                <h1>@Response.StatusCode</h1>
                <h3 class="text-uppercase">Hata Sebebi : @ViewBag.Kaynak</h3>
                <p class="text-muted m-t-30 m-b-30">Anasayfaya dönmeye çalışıyorsun gibi gözüküyor...</p>
                <a href="/Admin/Home/Index" class="btn btn-info btn-rounded waves-effect waves-light m-b-40">Anasayafaya Dön</a>
            </div>
            <footer class="footer text-center">2017 © Elite Admin.</footer>
        </div>
    </section>
    @Scripts.Render("~/bundles/adminscripts")
</body>
</html>
